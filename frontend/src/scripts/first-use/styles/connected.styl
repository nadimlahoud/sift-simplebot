  padding: 0;
}
.steps-component .steps-navigator li {
  display: inline-block;
  width: 0.3em;
  height: 0.3em;
  background: #d3d3d3;
  margin: 0.3em;
  transform: rotate(45deg);
  border: 1px solid #d3d3d3;
}
.steps-component .steps-navigator li.active-step {
  background: #fff;
