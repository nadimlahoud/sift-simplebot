{
  "name": "sift-simplebot",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run build:es && npm run build:umd-dev && npm run build:umd-prod && npm run build:css",
    "build:es": "npm run makepkg && node_modules/.bin/rollup -c config/bundle/es.js",
    "build:umd-dev": "npm run makepkg && node_modules/.bin/rollup -c config/bundle/umd-dev.js",
    "build:umd-prod": "npm run makepkg && node_modules/.bin/rollup -c config/bundle/umd-prod.js",
    "build:css": "gulp",
    "makepkg": "node_modules/.bin/json2module package.json > package.js",
    "clean": "rm -rf dist && rm package.js",
    "version": "npm run build && git add -A .",
    "postversion": "git push --tags origin HEAD",
    "start": "node server.js",
    "lint": "./node_modules/.bin/eslint index.js ./src/* ./examples/*",
    "pretest": "npm outdated",
    "test": "./node_modules/.bin/babel-tape-runner 'test/**/*-test.js' | ./node_modules/.bin/faucet && npm run lint"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "@redsift/redsift-bundler": "0.5.2",
    "@redsift/sift-sdk-web": "0.5.2",
    "gulp": "^3.9.1",
    "json2module": "0.0.3",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-tape-runner": "^2.0.1",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-plugin-react": "^6.1.2",
    "faucet": "0.0.1",
    "json-loader": "^0.5.4",
    "json2module": "0.0.3",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "rollup": "^0.34.10",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-eslint": "^2.0.2",
    "rollup-plugin-filesize": "^0.4.4",
    "rollup-plugin-hypothetical": "^1.2.1",
    "rollup-plugin-json": "^2.0.1",
    "rollup-plugin-node-globals": "^1.0.6",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rupture": "^0.6.1"
  }
}
